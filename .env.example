# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/monteeverest

# Payment Gateway
PAGARME_API_KEY=your_pagarme_api_key_here
PAGARME_ENCRYPTION_KEY=your_pagarme_encryption_key_here

# Session Secret (generate a random string)
SESSION_SECRET=your_super_secret_session_key_here

# Replit specific (only needed for Replit deployment)
REPL_ID=your_repl_id
REPLIT_DOMAINS=your-domain.replit.app

# Object Storage (optional)
PUBLIC_OBJECT_SEARCH_PATHS=/bucket/public
PRIVATE_OBJECT_DIR=/bucket/private
DEFAULT_OBJECT_STORAGE_BUCKET_ID=your_bucket_id